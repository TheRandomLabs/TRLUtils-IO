<FindBugsFilter>
	<Match>
		<Bug pattern="PATH_TRAVERSAL_IN"/>
	</Match>

	<Match>
		<Bug pattern="EXS_EXCEPTION_SOFTENING_RETURN_FALSE"/>
	</Match>

	<Match>
		<Bug pattern="DMI_HARDCODED_ABSOLUTE_FILENAME"/>
	</Match>

	<Match>
		<Class name="~.*Test"/>

		<Not>
			<Bug code="IJU"/>
		</Not>
	</Match>
</FindBugsFilter>
